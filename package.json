    "@fortawesome/react-fontawesome": "^0.1.4",
    "@hot-loader/react-dom": "^16.11.0",
    "availity-reactstrap-validation": "^2.6.1",
    "aws-sdk": "^2.610.0",
    "axios": "^0.19.2",
    "babel-loader": "^8.0.6",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
