{
  "name": "nano-react-app-template",
  "version": "0.1.0",
  "description": "Hard to get more minimal than this React app",
  "scripts": {
    "start": "webpack-dev-server --open",
    "client:dev": "webpack-dev-server --open --config webpack.dev.js",
    "build": "webpack --config webpack.prod.js  --display-error-details",
    "dev": "nodemon server/index.js",
    "dev:db": "docker-compose up",
    "worker": "nodemon worker/index.js",
    "docker:db": "docker exec -it mongodb mongo -u root -p example"
  },
  "license": "MIT",
